{
  "title": "My first dashboard",
  "titleVisible": false,

  "description": "My first dashboard description goes here",

  "layout": {
    "customJS" : ["jquery.peity.js", "chart.js"],
    "widgets" : [
      {"row" : 1, "col" : 1, "width" : 2, "height" : 1, "widget" : "linegraph",    "job" : "sales-graph",         "config": "sales"},
      {"row" : 1, "col" : 3, "width" : 2, "height" : 2, "widget" : "image",        "job" : "picture-of-the-day",  "config": "picture"},
      {"row" : 1, "col" : 5, "width" : 2, "height" : 4, "widget" : "quotes",       "job" : "quotes",              "config": "quotes-famous"},
      {"row" : 2, "col" : 1, "width" : 2, "height" : 3, "widget" : "keyvaluelist", "job" : "issue-types",         "config": "issue-types"},
      {"row" : 3, "col" : 2, "width" : 2, "height" : 2, "widget" : "calendar",     "job" : "google-calendar",     "config": "calendar-holidays"},
      {"row" : 4, "col" : 2, "width" : 2, "height" : 2, "widget" : "project",      "job" : "project",              "config": "project"},
      {"row" : 7, "col" : 1, "width" : 2, "height" : 2, "widget" : "point_scale",   "job" : "point_scale",             "config": "point_scale"},
      {"row" : 6, "col" : 2, "width" : 2, "height" : 2, "widget" : "iteration_length",     "job" : "iteration_length",  "config": "iteration_length"},
      {"row" : 5, "col" : 2, "width" : 2, "height" : 2, "widget" : "week_start_day",       "job" : "week_start_day",    "config": "week_start_day"},
      {"row" : 8, "col" : 2, "width" : 2, "height" : 2, "widget" : "version",              "job" : "version",               "config": "version"},
      {"row" : 9, "col" : 5, "width" : 2, "height" : 2, "widget" : "initial_velocity",     "job" : "initial_velocity",       "config": "initial_velocity"},
      {"row" : 10, "col" : 1, "width" : 2, "height" : 2, "widget" : "number_of_done_iterations_to_show",     "job" : "number_of_done_iterations_to_show",   "config": "number_of_done_iterations_to_show"}
    ]
  },

  "config" : {

    "calendar-holidays": {
      "interval": 60000,
      "calendarUrl": "http://www.calendarlabs.com/templates/ical/US-Holidays.ics",
      "maxEntries": 9,
      "widgetTitle": "Upcoming US Holidays"
    },
    "picture": {
      "interval": 12000,
      "widgetTitle": "Picture of the Day",
      "headshot": "headshot",
      "name": "name",
      "jobTitle": "jobTitle",
      "url":"https://www.willowtreeapps.com/api/v1.0/profiles"
    },
      "project": {
        "interval": 3600000,
        "widgetTitle": "Project",
        "name":"Test Project",
        "project_type": "private",
        "created_at":"2018-10-31T15:02:15Z",
        "url":"https://www.pivotaltracker.com/services/v5/projects/2221411",
        "id":2221411
},
"iteration_length": {
  "interval": 3600000,
  "widgetTitle": "Itertation Length"
},
"point_scale": {
  "interval": 3600000,
   "Point Scale":"2/4"
},
"number_of_done_iterations_to_show": {
  "interval": 3600000,
  "number_of_done_iterations_to_show":"12"
},

"week_start_day": {
  "interval": 3600000,
  "version":48
},
"number_of_done_iterations_to_show": {
  "interval": 3600000,
  "number_of_done_iterations_to_show":"12"
},
"initial_velocity": {
  "interval": 3600000,
  "initial_velocity":10
},
    "issue-types": {
      "interval": 60000,
      "widgetTitle": "Issues Types in the Project",
      "database": {
        "host": "mydatabase.mycompany.com",
        "port": 1000,
        "database": "salesdb"
      }
    },
    "quotes-famous": {
      "interval": 60000,
      "widgetTitle": "Famous Quotes",
      "quotes": [
        {
          "quote": "It's not that I'm so smart, it's just that I stay with problems longer.",
          "author": "Albert Einstein"
        },
        {
          "quote": "Ignorance is the curse of God; knowledge is the wing wherewith we fly to heaven.",
          "author": "William Shakespeare"
        },
        {
          "quote": "Software built on pride and love of subject is superior to software built for profit.",
          "author": "Ravi Simhambhatla"
        },
        {
          "quote": "If developersâ€™ pains are Java and .NET, the antidote is dynamic languages and frameworks.",
          "author": "Jeffrey Hammond"
        },
        {
          "quote": "Software is like sex: it's better when it's free.",
          "author": "Linus Torvalds"
        },
        {
          "quote": "People enjoy the interaction on the Internet, and the feeling of belonging to a group that does something interesting: that's how some software projects are born.",
          "author": "Linus Torvalds"
        },
        {
          "quote": "The ultimate search engine would basically understand everything in the world, and it would always give you the right thing.",
          "author": "Larry Page"
        },
        {
          "quote": "Mathematics is the queen of the sciences and number theory is the queen of mathematics.",
          "author": "Carl Friedrich Gauss"
        },
        {
          "quote": "Computer science is no more about computers than astronomy is about telescopes.",
